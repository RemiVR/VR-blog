<%= form_for :project, url: projects_path do |f| %>
	<p>
		<%= f.label :title %>
		<%= f.text_field :title %>
	</p>	
	
	<p>
		<%= f.label :link %>
		<%= f.text_field :link %>
	</p>

	<p>
		<%= f.label :description %>
		<%= f.text_area :description%>
	</p>

	<p>
		<%= f.submit "Submit"%>
	</p>

<% end %>